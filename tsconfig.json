{
	"compilerOptions": {
		"target": "ES6",
		"module": "CommonJS",
		//  "module": "ESNext",
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"typeRoots": ["./node_modules/@types"],
		"types": ["jest"]
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}
